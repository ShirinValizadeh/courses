<p id="notice"><%= notice %></p>
<div class="container">

<h3>Postings</h3>

<table  class="table table-hover">
  
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  

  <tbody>
    <% @postings.each do |posting| %>
      <tr>
        <td><%= posting.title %></td>
        <td><%= posting.description %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>


