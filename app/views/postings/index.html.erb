<p id="notice"><%= notice %></p>

<h1>Postings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postings.each do |posting| %>
      <tr>
        <td><%= posting.title %></td>
        <td><%= posting.description %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>


